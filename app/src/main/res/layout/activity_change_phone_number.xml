<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="changePhoneNumberVM" type="com.andhikaihsansapplication.app.modules.changephonenumber.data.viewmodel.ChangePhoneNumberVM"/>
    </data>
    <FrameLayout android:id="@+id/frameChangePhoneNu"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_800_7f">
        <View
            android:background="@drawable/rectangle_bg_red_700_radius_10"
            android:id="@+id/viewRectangleSeven"
            android:layout_gravity="center"
            android:layout_height="@dimen/_132pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_35pxh"
            android:layout_marginStart="@dimen/_35pxh"
            android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_290pxh"/>
        <LinearLayout android:id="@+id/linearColumnlineeleven"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_40pxh"
            android:layout_marginVertical="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <EditText
                android:id="@+id/lineLineEleven"
                android:layout_gravity="center"
                android:layout_height="@dimen/_40pxv"
                android:layout_width="@dimen/_258pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:backgroundTint="@color/white"
                android:inputType="phone"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnChangePhoneNumber"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_45pxh"
                android:layout_marginStart="@dimen/_45pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_9pxh"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@string/msg_change_phone_nu"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_4" tools:text="@string/msg_change_phone_nu"/>
        </LinearLayout>
    </FrameLayout>
</layout>
